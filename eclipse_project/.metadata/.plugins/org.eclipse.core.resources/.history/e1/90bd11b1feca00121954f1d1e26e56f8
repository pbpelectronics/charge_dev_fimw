  48:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** /* SPI SPE mask */
  49:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR1_SPE_Set          ((uint16_t)0x0040)
  50:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR1_SPE_Reset        ((uint16_t)0xFFBF)
  51:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** 
  52:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** /* I2S I2SE mask */
  53:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define I2SCFGR_I2SE_Set     ((uint16_t)0x0400)
  54:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define I2SCFGR_I2SE_Reset   ((uint16_t)0xFBFF)
  55:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** 
  56:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** /* SPI CRCNext mask */
  57:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR1_CRCNext_Set      ((uint16_t)0x1000)
  58:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** 
  59:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** /* SPI CRCEN mask */
  60:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR1_CRCEN_Set        ((uint16_t)0x2000)
  61:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR1_CRCEN_Reset      ((uint16_t)0xDFFF)
  62:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** 
  63:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** /* SPI SSOE mask */
  64:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR2_SSOE_Set         ((uint16_t)0x0004)
  65:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR2_SSOE_Reset       ((uint16_t)0xFFFB)
  66:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** 
  67:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** /* SPI registers Masks */
  68:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define CR1_CLEAR_Mask       ((uint16_t)0x3040)
  69:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #define I2SCFGR_CLEAR_Mask   ((uint16_t)0xF040)
  70:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** 
  71:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** /* SPI or I2S mode selection masks */
