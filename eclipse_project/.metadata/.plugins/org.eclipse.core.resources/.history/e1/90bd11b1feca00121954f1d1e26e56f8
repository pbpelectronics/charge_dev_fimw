 283:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** 
 284:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       /* Get the value of the PLL3 multiplier */      
 285:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       if((tmp > 5) && (tmp < 15))
 286:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       {
 287:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****         /* Multiplier is between 8 and 14 (value 15 is forbidden) */
 288:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****         tmp += 2;
 289:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       }
 290:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       else
 291:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       {
 292:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****         if (tmp == 15)
 293:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****         {
 294:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****           /* Multiplier is 20 */
 295:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****           tmp = 20;
 296:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****         }
 297:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       }      
 298:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       /* Get the PREDIV2 value */
 299:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       sourceclock = (uint32_t)(((RCC->CFGR2 & I2S_DIV_MASK) >> 4) + 1);
 300:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       
 301:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       /* Calculate the Source Clock frequency based on PLL3 and PREDIV2 values */
 302:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       sourceclock = (uint32_t) ((HSE_Value / sourceclock) * tmp * 2); 
 303:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****     }
 304:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****     else
 305:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****     {
 306:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       /* I2S Clock source is System clock: Get System Clock frequency */
 307:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       RCC_GetClocksFreq(&RCC_Clocks);      
 308:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       
 309:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       /* Get the source clock value: based on System Clock value */
 310:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****       sourceclock = RCC_Clocks.SYSCLK_Frequency;
 311:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****     }        
 312:../libs/StdPeripheralDriver/src/stm32f10x_spi.c **** #else /* STM32F10X_HD */
 313:../libs/StdPeripheralDriver/src/stm32f10x_spi.c ****     /* I2S Clock source is System clock: Get System Clock frequency */
