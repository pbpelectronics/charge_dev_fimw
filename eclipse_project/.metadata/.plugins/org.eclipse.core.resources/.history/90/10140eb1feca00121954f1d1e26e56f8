  45:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
  46:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /* Flash Access Control Register bits */
  47:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define ACR_LATENCY_Mask         ((uint32_t)0x00000038)
  48:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define ACR_HLFCYA_Mask          ((uint32_t)0xFFFFFFF7)
  49:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define ACR_PRFTBE_Mask          ((uint32_t)0xFFFFFFEF)
  50:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
  51:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /* Flash Access Control Register bits */
  52:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define ACR_PRFTBS_Mask          ((uint32_t)0x00000020) 
  53:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
  54:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /* Flash Control Register bits */
  55:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_PG_Set                ((uint32_t)0x00000001)
  56:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_PG_Reset              ((uint32_t)0x00001FFE) 
  57:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_PER_Set               ((uint32_t)0x00000002)
  58:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_PER_Reset             ((uint32_t)0x00001FFD)
  59:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_MER_Set               ((uint32_t)0x00000004)
  60:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_MER_Reset             ((uint32_t)0x00001FFB)
  61:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_OPTPG_Set             ((uint32_t)0x00000010)
  62:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_OPTPG_Reset           ((uint32_t)0x00001FEF)
  63:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_OPTER_Set             ((uint32_t)0x00000020)
  64:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_OPTER_Reset           ((uint32_t)0x00001FDF)
  65:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_STRT_Set              ((uint32_t)0x00000040)
  66:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define CR_LOCK_Set              ((uint32_t)0x00000080)
  67:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
  68:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /* FLASH Mask */
  69:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define RDPRT_Mask               ((uint32_t)0x00000002)
  70:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define WRP0_Mask                ((uint32_t)0x000000FF)
  71:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define WRP1_Mask                ((uint32_t)0x0000FF00)
  72:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define WRP2_Mask                ((uint32_t)0x00FF0000)
  73:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define WRP3_Mask                ((uint32_t)0xFF000000)
  74:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define OB_USER_BFB2             ((uint16_t)0x0008)
  75:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
  76:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /* FLASH Keys */
  77:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define RDP_Key                  ((uint16_t)0x00A5)
  78:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define FLASH_KEY1               ((uint32_t)0x45670123)
  79:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define FLASH_KEY2               ((uint32_t)0xCDEF89AB)
  80:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
  81:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /* FLASH BANK address */
  82:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define FLASH_BANK1_END_ADDRESS   ((uint32_t)0x807FFFF)
  83:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
  84:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /* Delay definition */   
  85:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define EraseTimeout          ((uint32_t)0x000B0000)
  86:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #define ProgramTimeout        ((uint32_t)0x00002000)
  87:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /**
