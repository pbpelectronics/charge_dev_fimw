1153:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** #ifdef STM32F10X_XL
1154:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** /**
1155:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   * @brief  Configures to boot from Bank1 or Bank2.  
1156:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   * @note   This function can be used only for STM32F10x_XL density devices.
1157:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   * @param  FLASH_BOOT: select the FLASH Bank to boot from.
1158:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *   This parameter can be one of the following values:
1159:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *     @arg FLASH_BOOT_Bank1: At startup, if boot pins are set in boot from user Flash
1160:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        position and this parameter is selected the device will boot from Bank1(Default).
1161:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *     @arg FLASH_BOOT_Bank2: At startup, if boot pins are set in boot from user Flash
1162:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        position and this parameter is selected the device will boot from Bank2 or Bank1,
1163:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        depending on the activation of the bank. The active banks are checked in
1164:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        the following order: Bank2, followed by Bank1.
1165:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        The active bank is recognized by the value programmed at the base address
1166:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        of the respective bank (corresponding to the initial stack pointer value
1167:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        in the interrupt vector table).
1168:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *        For more information, please refer to AN2606 from www.st.com.    
1169:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   * @retval FLASH Status: The returned value can be: FLASH_ERROR_PG, 
1170:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   *         FLASH_ERROR_WRP, FLASH_COMPLETE or FLASH_TIMEOUT.
1171:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   */
1172:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** FLASH_Status FLASH_BootConfig(uint16_t FLASH_BOOT)
1173:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** { 
1174:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   FLASH_Status status = FLASH_COMPLETE; 
1175:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   assert_param(IS_FLASH_BOOT(FLASH_BOOT));
1176:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   /* Authorize the small information block programming */
1177:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   FLASH->OPTKEYR = FLASH_KEY1;
1178:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   FLASH->OPTKEYR = FLASH_KEY2;
1179:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   
1180:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   /* Wait for last operation to be completed */
1181:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   status = FLASH_WaitForLastOperation(ProgramTimeout);
1182:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   
1183:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   if(status == FLASH_COMPLETE)
1184:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   {  
1185:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     /* Enable the Option Bytes Programming operation */
1186:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     FLASH->CR |= CR_OPTPG_Set; 
1187:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** 
1188:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     if(FLASH_BOOT == FLASH_BOOT_Bank1)
1189:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     {
1190:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****       OB->USER |= OB_USER_BFB2;
1191:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     }
1192:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     else
1193:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     {
1194:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****       OB->USER &= (uint16_t)(~(uint16_t)(OB_USER_BFB2));
1195:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     }
1196:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     /* Wait for last operation to be completed */
1197:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     status = FLASH_WaitForLastOperation(ProgramTimeout);
1198:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     if(status != FLASH_TIMEOUT)
1199:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     {
1200:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****       /* if the program operation is completed, disable the OPTPG Bit */
1201:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****       FLASH->CR &= CR_OPTPG_Reset;
1202:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****     }
1203:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   }    
1204:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   /* Return the Option Byte program Status */
1205:../libs/StdPeripheralDriver/src/stm32f10x_flash.c ****   return status;
1206:../libs/StdPeripheralDriver/src/stm32f10x_flash.c **** }
